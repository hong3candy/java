
public class Control {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 0, b = 15;
		
		if ( a > 10 ){
			System.out.println("a="+a);
		}
		else {
			a=0;
			System.out.println(a);
		}
		System.out.println("----------");
		if ( b > 10 ) {
			System.out.println("b="+b);
		}
		else {
			b<10;
			System.out.println(b);
		}
	}

}
